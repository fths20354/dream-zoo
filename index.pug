        //-整個頁面的背景圖
        div(class="background")
            //-大門 地圖 熱氣球 告示牌
            //-include Logo.pug 門扉
            include logo.pug
            //-include nav.pug 熱氣球
            include nav.pug
            //-門框
            div(id="door")
                //-觀賞園區
                div(id="link-animal-park")
                    a(href="")
                //-互動園區
                div(id="link-active-park")
                //-主題咖啡廳
                div(id="link-coffee")
                //-精品禮品店
                div(id="link-present")
                //-活動展覽
                div(id="link-event")
                //-告示牌-關於我們-聯絡店家-店家位置-FB-LINE
                div(id="contact")
            include leading.pug
            //-地面-版權聲明
            include footer.pug



